    <body>
        <div class="row w-100 p-3 align-items-center justify-content-center">
            <div class="col-xl-6 col-lg-6 col-md-6 col-sm-6 col-12">
                <div class="card" style="border: none;">
                    <img class="card-img-top" src="diseno/image/icon2.png" alt="Card image cap">
                    <div class="card-body">
                        <h5 class="card-title text-danger">Calls</h5>
                        <h6 class="card-subtitle mb-2 text-success text-muted">Register System</h6>
                        <p class="card-text">Have fun <i class="fab fa-jenkins"></i></p>
                        <div class="row">
                            <div class="col">
                                <div class="form-group">
                                    <label for="exampleInputEmail1">Email address</label>
                                    <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter email">
